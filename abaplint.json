{  
  "global": {
    "files": "/src/**/*.*",
    "applyUnspecifiedRules": false
  },
  "dependencies": [],
  "syntax": {
    "version": "v702",
    "errorNamespace": "^(Z|Y)",
    "globalConstants": [],
    "globalMacros": []
  },
  "rules": {  
    "7bit_ascii":{  
      "enabled": true
    },
    "contains_tab":{  
      "enabled": true
    },
    "functional_writing":{  
      "enabled": true,
      "ignoreExceptions": true
    },
    "line_length":{  
      "enabled": true,
      "length": 130
    },
    "max_one_statement":{  
      "enabled": true
    },
    "parser_error":{  
      "enabled": true
    },
    "space_before_colon":{  
      "enabled": true
    },
    "colon_missing_space":{  
      "enabled": true
    },
    "exit_or_check":{  
      "enabled": true
    },
    "line_only_punc":{  
      "enabled": true,
      "ignoreExceptions": true
    },
    "obsolete_statement":{  
      "enabled": true
    },
    "start_at_tab":{  
      "enabled": true
    },
    "whitespace_end":{  
      "enabled": true
    },
    "exporting":{  
      "enabled": true
    },
    "empty_statement":{  
      "enabled": true
    },
    "sequential_blank":{  
      "enabled": true,
      "lines":4
    },
    "indentation":{  
      "enabled": true,
      "alignTryCatch": true,
      "ignoreExceptions": true
    }
   }
}
